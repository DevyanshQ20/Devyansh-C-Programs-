#include <stdio.h>
#include <string.h>
int main()
{
 char str[] = { "abbba" };
 int l = 0;
 int h = strlen(str) - 1;
 while (h > l) {
 if (str[l++] != str[h--]) {
 printf("%s is not a palindrome\n", str);
 return 0;
}
 }
 printf("%s is a palindrome\n", str);
 return 0;
}
45. C program to count frequency of each character in a string.
#include<stdio.h>
int main()
{
 char name[30];
 printf("Enter name: ");
 gets(name); 
 printf("Name: ");
 puts(name); 
return 0;
}
